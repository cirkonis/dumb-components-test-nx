{
  "root": "apps/shared-design-components-test-e2e",
  "sourceRoot": "apps/shared-design-components-test-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/shared-design-components-test-e2e/cypress.json",
        "devServerTarget": "shared-design-components-test:storybook"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "shared-design-components-test:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/shared-design-components-test-e2e/**/*.{js,ts}"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["shared-design-components-test"]
}
