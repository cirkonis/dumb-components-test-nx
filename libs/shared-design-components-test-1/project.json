{
  "projectType": "library",
  "root": "libs/shared-design-components-test-1",
  "sourceRoot": "libs/shared-design-components-test-1/src",
  "prefix": "dumb-components-test",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared-design-components-test-1"],
      "options": {
        "jestConfig": "libs/shared-design-components-test-1/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared-design-components-test-1/src/**/*.ts",
          "libs/shared-design-components-test-1/src/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "config": {
          "configFolder": "libs/shared-design-components-test-1/.storybook"
        },
        "projectBuildConfig": "shared-design-components-test:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "styles": ["libs/shared-design-components-test-1/src/styles.css"],
        "uiFramework": "@storybook/angular",
        "outputPath": "dist/storybook/shared-design-components-test",
        "config": {
          "configFolder": "libs/shared-design-components-test-1/.storybook"
        },
        "projectBuildConfig": "shared-design-components-test:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
